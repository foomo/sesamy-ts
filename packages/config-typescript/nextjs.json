{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js",
	"extends": "./base.json",
	"compilerOptions": {
		"noUnusedLocals": false,
		"noImplicitOverride": false,

		"module": "ESNext",
		"moduleResolution": "bundler",

		"noEmit": true,

		"jsx": "preserve",

		"plugins": [{ "name": "next" }],
		"lib": ["DOM", "DOM.Iterable", "ES2015"],

		"target": "ES2015"
	}
}
