{
	"version": "0.1.0",
	"name": "@foomo/sesamy",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "pnpm run build:client && pnpm run build:server",
		"build:client": "tsup --config tsup.client.ts",
		"build:server": "tsup --config tsup.server.ts",
		"type-check": "tsc --noEmit"
	},
	"files": ["dist"],
	"types": "./dist/server/index.client.d.ts",
	"exports": {
		".": {
			"edge": {
				"types": "./dist/client/index.client.d.ts",
				"default": "./dist/client/index.client.js"
			},
			"edge-light": {
				"types": "./dist/client/index.client.d.ts",
				"default": "./dist/client/index.client.js"
			},
			"browser": {
				"types": "./dist/client/index.client.d.ts",
				"default": "./dist/client/index.client.js"
			},
			"worker": {
				"types": "./dist/client/index.client.d.ts",
				"default": "./dist/client/index.client.js"
			},
			"workerd": {
				"types": "./dist/client/index.client.d.ts",
				"default": "./dist/client/index.client.js"
			},
			"import": {
				"types": "./dist/server/index.server.d.ts",
				"default": "./dist/server/index.server.js"
			},
			"node": {
				"types": "./dist/server/index.server.d.ts",
				"default": "./dist/server/index.server.js"
			},
			"default": "./dist/server/index.client.js"
		}
	},
	"dependencies": {},
	"devDependencies": {
		"@repo/typescript-config": "workspace:*",
		"@types/gtag.js": "0.0.19",
		"@types/node": "^20",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"resolve-glob": "latest",
		"typescript": "^5",
		"tsup": "latest"
	},
	"peerDependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0"
	}
}
