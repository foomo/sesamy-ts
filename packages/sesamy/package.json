		"react-dom": "18.2.0",
		"typescript": "^5",
		"tsx": "latest",
		"rimraf": "latest",
		"esbuild": "latest"
	},
	"peerDependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0"
